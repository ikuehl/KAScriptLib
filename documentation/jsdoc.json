{
    "plugins": ["plugins/eaScriptLib"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
  "templates": {
    "default": {"layoutFile": ""},
    "cleverLinks": false,
    "monospaceLinks": false
  }
}